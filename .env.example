# Environment variables for AI-Powered Resume Relevance Checker

# OpenAI API Key (optional - app will use fallback analysis if not provided)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Flask Secret Key (change this in production)
SECRET_KEY=your_secret_key_for_production

# Flask Debug Mode (set to False in production)
FLASK_DEBUG=True

# Flask Environment
FLASK_ENV=development